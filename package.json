{
  "name": "@fzed51/react-ui",
  "version": "0.1.1",
  "description": "Ensemble de composant reactjs",
  "main": "./dist/index.js",
  "scripts": {
    "_declaration": "tsc -d --emitDeclarationOnly --jsx react-jsx --outDir ./dist/ --strict",
    "_build": "npm run build && npm run _declaration",
    "_publish": "npm run _build && npm publish && git push && git push --tags",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:d": "webpack --env mode=dev",
    "build": "webpack --env mode=production",
    "publish:pat": "npm version patch && npm run _publish",
    "publish:min": "npm version minor && npm run _publish",
    "publish:maj": "npm version major && npm run _publish",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "ux"
  ],
  "author": "Fabien Sanchez <fzed51+git@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-json-strings": "^7.16.0",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "netlify-cli": "^8.1.5",
    "sass": "^1.44.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
